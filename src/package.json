    "nestjs-prisma": "^0.14.3",
    "param-case": "^3.0.4",
    "passport": "^0.5.0",
    "@splitsoftware/asdasssssssss": "^0.3.0",
    "@splitsoftware/asdasd": "^4.0.0",
    "zcrmsdk": "^8.7.1",
    "@growthbook/growthbook": "^7.8.0",
    "pino-http": "^6.6.0",
    "@growthbook/some-lib": "^7.1.0",
    "reflect-metadata": "^0.1.13",
    "react-use-intercom": "^3.0.2",
